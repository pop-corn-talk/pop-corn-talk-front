{
  "parser": "@babel/eslint-parser",
  "plugins": ["react"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser":true,
    "node":true
  },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
